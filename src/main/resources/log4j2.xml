<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="WARN">
	<Appenders>
		<Console name="Console" target="SYSTEM_OUT">
			<PatternLayout pattern="%d{yyyy-MM-dd HH:mm:ss,SSS} [%t] %highlight{%-5level} - %msg%n"
						   disableAnsi="false"/>
		</Console>
	</Appenders>
	<Loggers>
		<Logger name="com.google.mail.qa" level="trace"/>
		<Root level="error">
			<AppenderRef ref="Console"/>
		</Root>
	</Loggers>
</Configuration>